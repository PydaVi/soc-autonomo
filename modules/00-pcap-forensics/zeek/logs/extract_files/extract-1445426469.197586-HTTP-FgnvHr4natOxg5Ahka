	function dipchange() {
		var dip = document.setting_form.DipSwitchCheckBox;
		var v1 = document.setting_form.v1;
		var v2 = document.setting_form.v2;

		if(dip.checked)
		{
			v1.disabled=true;
			v2.disabled=true;
		}
		else
		{
			v1.disabled=false;
			v2.disabled=false;
		}
