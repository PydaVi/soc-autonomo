<script type="text/javascript">
var GatewayMode = 1;
function tgm(){
var tbl = document.getElementById("TableStatus");var x=tbl.rows;var state;
if (GatewayMode == 1){GatewayMode=2; state = true;}
else{GatewayMode=1;state = false;}
for (i = 1; i < x.length; i++){var y = x[i].cells; y[4].childNodes[0].disabled=state;}
if (GatewayMode == 2) assignAddresses();
}
function assignAddresses(){
var tbl = document.getElementById("TableStatus"); var vals=new Array(0,0,0,0);var x=tbl.rows
for (i = 1; i <= x.length; i++){var y = x[i].cells
if (y[0].childNodes[0].value != 0){
var idx = parseInt(y[0].childNodes[0].value, 10)-1;
y[4].childNodes[0].value = vals[idx];
vals[idx] = vals[idx] + parseInt(y[3].childNodes[0].value, 10);}
else{y[4].childNodes[0].value = 0;}}}
function c(){if (GatewayMode == 2) assignAddresses();}
