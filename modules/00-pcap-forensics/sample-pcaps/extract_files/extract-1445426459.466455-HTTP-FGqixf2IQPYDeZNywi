	function SpeedSet(j){
			
		k=eval(document.getElementById("isfiber"+j).value);
		l=eval(document.getElementById("portidx"+j).value);
		j=j-1;
		if(k==1){
			document.getElementsByTagName("select")[j*3].disabled=true;
			document.getElementsByTagName("select")[j*3].style.backgroundColor="#F5F5F5";
			
			document.getElementsByTagName("select")[j*3+2].disabled=true;
