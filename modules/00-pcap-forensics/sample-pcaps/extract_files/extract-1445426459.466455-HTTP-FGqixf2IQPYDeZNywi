				document.getElementsByTagName("select")[j*3+1].disabled=false;
				document.getElementsByTagName("select")[j*3+1].style.backgroundColor="#FFFFFF";
			}else{
				document.getElementsByTagName("select")[j*3+1].disabled=true;
				document.getElementsByTagName("select")[j*3+1].style.backgroundColor="#F5F5F5";					
			}						

		}else{
			document.getElementsByTagName("select")[j*3].disabled=false;
			document.getElementsByTagName("select")[j*3].style.backgroundColor="#FFFFFF";			
