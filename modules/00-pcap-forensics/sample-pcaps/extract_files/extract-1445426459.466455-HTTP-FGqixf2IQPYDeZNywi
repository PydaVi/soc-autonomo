				document.getElementsByTagName("select")[j*3+2].style.backgroundColor="#FFFFFF";
		  }							
		}				
	}

	
	function SpeedSelect(j){
			
		k=eval(document.getElementById("isfiber"+j).value);
		l=eval(document.getElementById("portidx"+j).value);
		j=j-1;
		if(k==1){
			document.getElementsByTagName("select")[j*3].disabled=true;
			document.getElementsByTagName("select")[j*3].style.backgroundColor="#F5F5F5";
			
			document.getElementsByTagName("select")[j*3+2].disabled=true;
			document.getElementsByTagName("select")[j*3+2].style.backgroundColor="#F5F5F5";						
			
			if( document.getElementsByTagName("select")[j*3].options[document.getElementsByTagName("select")[j*3].selectedIndex].value==5 ){
				document.getElementsByTagName("select")[j*3+1].disabled=false;
				document.getElementsByTagName("select")[j*3+1].style.backgroundColor="#FFFFFF";
			}else{
				document.getElementsByTagName("select")[j*3+1].disabled=true;
				document.getElementsByTagName("select")[j*3+1].style.backgroundColor="#F5F5F5";					
			}
		}else{
			document.getElementsByTagName("select")[j*3].disabled=false;
			document.getElementsByTagName("select")[j*3].style.backgroundColor="#FFFFFF";			
			if( document.getElementsByTagName("select")[j*3].options[document.getElementsByTagName("select")[j*3].selectedIndex].value==2 ||
			    document.getElementsByTagName("select")[j*3].options[document.getElementsByTagName("select")[j*3].selectedIndex].value==1 ||
			    document.getElementsByTagName("select")[j*3].options[document.getElementsByTagName("select")[j*3].selectedIndex].value==4 ||
			    document.getElementsByTagName("select")[j*3].options[document.getElementsByTagName("select")[j*3].selectedIndex].value==3  ){
				document.getElementsByTagName("select")[j*3+1].disabled=true;
				document.getElementsByTagName("select")[j*3+1].selectedIndex=1;
				document.getElementsByTagName("select")[j*3+1].style.backgroundColor="#F5F5F5";
			}else{
				document.getElementsByTagName("select")[j*3+1].disabled=false;
				// ken modified. flow control is default off for all ports.  2006.11.30.  
				//document.getElementsByTagName("select")[j*3+1].selectedIndex=0;
				document.getElementsByTagName("select")[j*3+1].selectedIndex=1;
				document.getElementsByTagName("select")[j*3+1].style.backgroundColor="#FFFFFF";				
			}
			if(document.getElementsByTagName("select")[j*3+2].selectId.options.length !=1){
				document.getElementsByTagName("select")[j*3+2].disabled=false;
				document.getElementsByTagName("select")[j*3+2].style.backgroundColor="#FFFFFF";
		}							
		}				
	}
	
	function touchLasttime()
	{
							theName = "lasttime";
					expires = null;
					now=new Date( );
					document.cookie =theName + "=" + now.getTime() + "; path=/" + ((expires == null) ? " " : "; expires = " +expires.toGMTString());

	}
	function CheckCookie()
	{
		
	}		
	function AccountDiff(){

		CheckCookie();
		for(i=0;i<document.getElementsByTagName("input").length/4;i++){
			if(document.getElementsByTagName("input")[i*4+2].disabled==false){
				PortEnable(i+1);
			}else{
				document.getElementsByTagName("select")[i*3].disabled=true;
				document.getElementsByTagName("select")[i*3].style.backgroundColor="#F5F5F5";

				document.getElementsByTagName("select")[i*3+1].disabled=true;
				document.getElementsByTagName("select")[i*3+1].style.backgroundColor="#F5F5F5";					
				
				document.getElementsByTagName("select")[i*3+2].disabled=true;
				document.getElementsByTagName("select")[i*3+2].style.backgroundColor="#F5F5F5";				
			}
		}
		
		theData = "";
		theName = "AccountName508=";
		theCookie = document.cookie+";";
		start = theCookie.indexOf(theName);
		if(start != -1){
			end=theCookie.indexOf(";",start);
			theData = unescape(theCookie.substring(start+theName.length,end));
		}		
		
		if(theData=="user"){
			for(i=0;i<document.getElementsByTagName("input").length;i++){
				document.getElementsByTagName("input")[i].disabled=true;
				if((i%2)==1){
					document.getElementsByTagName("input")[i].style.backgroundColor="#F5F5F5";
				}
			}
			for(i=0;i<document.getElementsByTagName("select").length;i++){
				document.getElementsByTagName("select")[i].disabled=true;
				document.getElementsByTagName("select")[i].style.backgroundColor="#F5F5F5";
			}			
		}		
				
	}	
