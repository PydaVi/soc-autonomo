function init_msg(str)
{
	if (str == "")
		return;
	else
		alert(str);
	return;
}

function init_conf_menu()
{
		init_conf("network_menu_");
		init_conf("serial_menu_");
		init_conf("menulist_menu_");
		init_conf("cluster_menu_");
		init_conf("power_menu_");
		init_conf("pccard_menu_");
		init_conf("status_menu_");
		init_conf("admin_menu_");
		init_conf("statistics_menu_");
}

function init_conf(midstr)
{
	var i=0;
	var runstr;
	var idstr;
	var elempara;

//	idstr = midstr + "0";
//	elempara = document.getElementById(idstr);
//	if (elempara.style.display != "block")

	if (!document.getElementById(midstr))
	{
		while(1)
		{
			idstr = midstr + i;
			elempara = document.getElementById(idstr);
			if (elempara)
			{	
				elempara.style.display = "none";	
				i++;
			}
			else
			{
				break;
			}
		}
	}
/*
	else
	{
		while(1)
		{
			idstr = midstr + i;
			alert(idstr);
			elempara = document.getElementById(idstr);
			if (elempara)
			{	
				elempara.style.display = "";	
				i++;
			}
			else
			{
				break;
			}
		}
	}
*/
}

function show_conf(time, midstr)
{
	var i=0;
	var runstr;
	var idstr;
	var elempara;

	idstr = midstr + "0";
	elempara = document.getElementById(idstr);

	if (elempara.style.display == "none")
	{
		while(1)
		{
			idstr = midstr + i;
			elempara = document.getElementById(idstr);
			if (elempara)
			{	
				runstr = "showsub(\"" + idstr + "\")";
				setTimeout(runstr, time + 50*i);
				i++;
			}
			else
			{
				break;
			}
		}
	}	
}

function close_conf(midstr)
{
	var i=0;
	var runstr;
	var idstr;
	var elempara;

	idstr = midstr + "0";
	elempara = document.getElementById(idstr);

	if (elempara.style.display != "none")
	{
		while(1)
		{
			idstr = midstr + i;
			elempara = document.getElementById(idstr);
			if (elempara)
			{	
				runstr = "hidesub(\"" + idstr + "\")";
				setTimeout(runstr, 50*i);
				i++;
			}
			else
			{
				break;
			}
		}
	}

	return 50*i;
}

function show_network_conf(time)
{
	if (document.getElementById("network_menu_0"))
		show_conf(time, "network_menu_");
}

function close_network_conf()
{
	if (document.getElementById("network_menu_0"))
		return close_conf("network_menu_");
	else
		return 0;
}


function slide_network_conf()
{
	var time = 0;
	var elempara = document.getElementById("network_menu_0");
//	time += close_network_conf();
	time += close_menulist_conf();
	time += close_serial_conf();
	time += close_cluster_conf();
	time += close_power_conf();
	time += close_pccard_conf();
	time += close_status_conf();
	time += close_admin_conf();
	time += close_statistics_conf();

	if (elempara.style.display == "none")
		show_network_conf(time);
	else
		close_network_conf();
}


function show_menulist_conf(time)
{
	if (document.getElementById("menulist_menu_0"))
		show_conf(time, "menulist_menu_");
}

function close_menulist_conf()
{
	if (document.getElementById("menulist_menu_0"))
		return close_conf("menulist_menu_");
	else
		return 0;
}

function slide_menulist_conf()
{
	var time = 0;
	var elempara = document.getElementById("menulist_menu_0");
	time += close_network_conf();

//	time += close_menulist_conf();
	time += close_serial_conf();

	time += close_cluster_conf();
	time += close_power_conf();
	time += close_pccard_conf();
	time += close_status_conf();
	time += close_admin_conf();
	time += close_statistics_conf();

	if (elempara.style.display == "none")
		show_menulist_conf(time);
	else
		close_menulist_conf();
}

